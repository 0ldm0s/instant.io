{
  "name": "instant.io",
  "description": "Secure, anonymous, streaming file transfer",
  "version": "1.0.0",
  "author": "Feross Aboukhadijeh <feross@feross.org> (http://feross.org/)",
  "bugs": {
    "url": "https://github.com/feross/instant.io/issues"
  },
  "dependencies": {
    "compression": "^1.0.9",
    "create-torrent": "^2.3.0",
    "debug": "^1.0.4",
    "drag-drop": "^1.0.2",
    "express": "^4.7.2",
    "hat": "0.0.3",
    "inherits": "^2.0.1",
    "jade": "^1.5.0",
    "node-static": "^0.7.3",
    "once": "^1.3.0",
    "parse-torrent": "^1.7.0",
    "posix": "^1.0.3",
    "simple-peer": "^1.0.0",
    "webtorrent-swarm": "^0.1.1",
    "webtorrent-tracker": "^0.1.0"
  },
  "devDependencies": {
    "browserify": "^4.1.11",
    "nib": "^1.0.3",
    "nodemon": "^1.2.1",
    "stylus": "^0.47.3",
    "watchify": "^1.0.1"
  },
  "homepage": "http://feross.org",
  "keywords": [
    "p2p",
    "webrtc",
    "data channel",
    "file transfer"
  ],
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/feross/instant.io.git"
  },
  "scripts": {
    "build": "npm run build-css && npm run build-js",
    "build-css": "stylus -u nib css/main.styl -o static/ -c",
    "build-js": "browserify client > static/bundle.js",
    "start": "node server",
    "watch": "npm run watch-css & npm run watch-js & DEBUG=webtorrent:* nodemon server -e js,jade -d 1",
    "watch-css": "stylus -u nib css/main.styl -o static/ -w",
    "watch-js": "watchify client -o static/bundle.js -dv"
  }
}
